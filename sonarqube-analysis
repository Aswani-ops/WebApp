node{
   stage('SCM Checkout'){
     git 'https://github.com/saurjain326/WebApp'
   }
      
   stage('SonarQube Analysis') {
        def mvnHome =  tool name: 'maven', type: 'maven'
        withSonarQubeEnv(credentialsId: 'sonarqubelocalA', installationName: 'sonarqubelocalA') { 
          sh "${mvnHome}/bin/mvn clean verify sonar:sonar -sonarqubelocalA.sources=. -sonarqubelocalA.tests=. -sonarqubelocalA.inclusions=**/test/java/servlet/createpage_junit.java -sonarqubelocalA.test.exclusions=**/test/java/servlet/createpage_junit.java"
        }
    }
}
